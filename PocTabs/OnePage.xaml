<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="PocTabs.OnePage"
             Title="One">
    <VerticalStackLayout>
        <Label Text="Dynamic 1" />
        <Label Text="{Binding Appeared, StringFormat='Appears: {0}'}" />
        <Label Text="{Binding Disappeared, StringFormat='Disappears: {0}'}" />
    </VerticalStackLayout>
</ContentPage>
